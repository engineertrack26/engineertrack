{
  "expo": {
    "name": "EngineerTrack",
    "slug": "engineertrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a73e8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.engineertrack.app",
      "infoPlist": {
        "NSCameraUsageDescription": "EngineerTrack needs camera access to take photos for daily logs.",
        "NSPhotoLibraryUsageDescription": "EngineerTrack needs photo library access to attach images to daily logs."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a73e8"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.engineertrack.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.NOTIFICATIONS",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "engineertrack",
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-notifications",
      "expo-localization",
      "expo-secure-store",
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "572a3922-f6f2-4d4c-90af-21df8d0645f3"
      }
    },
    "owner": "engineer-track"
  }
}
